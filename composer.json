{
    "name": "giggsey/libphonenumber-for-php",
    "type": "library",
    "description": "Unofficial PHP Port of Google's libphonenumber",
    "keywords": ["phonenumber", "libphonenumber", "mobile", "validation"],
    "homepage": "https://github.com/giggsey/libphonenumber-for-php",
    "license": "Apache-2.0",
    "replace": {
        "practo/libphonenumber-for-php": "*"
    },
    "autoload": {
        "psr-0": {
            "libphonenumber": "src/"
        }
    },
    "archive": {
        "exclude": ["Tests/", "build/", "/.travis.yml", "/updateMetadata.sh", "/build.xml"]
    },
    "require-dev": {
        "phpunit/phpunit": "3.7"
    }
}
